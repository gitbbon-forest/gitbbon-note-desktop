{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ES2020",
		"lib": ["ES2020", "DOM"],
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true
	},
	"include": [
		"./**/*"
	]
}
